{
  "expo": {
    "name": "PlantApp",
    "slug": "PlantApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "plantapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.PlantApp"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.PlantApp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Rubik-Light.ttf",
            "./assets/fonts/Rubik-Regular.ttf",
            "./assets/fonts/Rubik-Medium.ttf",
            "./assets/fonts/Rubik-SemiBold.ttf",
            "./assets/fonts/Rubik-Bold.ttf",
            "./assets/fonts/Rubik-ExtraBold.ttf",
            "./assets/fonts/Rubik-Black.ttf",
            "./assets/fonts/Rubik-Italic.ttf",
            "./assets/fonts/SFProText-Bold.ttf"
          ],
          "android": {
            "fonts": [
              {
                "fontFamily": "RubikLight",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/Rubik-Light.ttf",
                    "weight": 300
                  }
                ]
              },
              {
                "fontFamily": "RubikRegular",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/Rubik-Regular.ttf",
                    "weight": 400
                  }
                ]
              },
              {
                "fontFamily": "RubikMedium",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/Rubik-Medium.ttf",
                    "weight": 500
                  }
                ]
              },
              {
                "fontFamily": "RubikSemiBold",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/Rubik-SemiBold.ttf",
                    "weight": 600
                  }
                ]
              },
              {
                "fontFamily": "RubikBold",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/Rubik-Bold.ttf",
                    "weight": 700
                  }
                ]
              },
              {
                "fontFamily": "RubikExtraBold",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/Rubik-ExtraBold.ttf",
                    "weight": 800
                  }
                ]
              },
              {
                "fontFamily": "RubikBlack",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/Rubik-Black.ttf",
                    "weight": 900
                  }
                ]
              },
              {
                "fontFamily": "RubikItalic",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/Rubik-Italic.ttf",
                    "style": "italic"
                  }
                ]
              },
              {
                "fontFamily": "SFProTextBold",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/SFProText-Bold.ttf",
                    "weight": 700
                  }
                ]
              }
            ]
          }
        }
      ]
    ],
    "experiments": {
      "reactCompiler": true
    },
    "extra": {
      "apiBaseUrl": "https://dummy-api-jtg6bessta-ey.a.run.app"
    }
  }
}
